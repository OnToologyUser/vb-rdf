    </owl:Class>
    


    <!-- http://www.semanticweb.org/veyis/ontologies/2024/9/vbc#VideoLearningMaterial -->


    <owl:Class rdf:about="http://www.semanticweb.org/veyis/ontologies/2024/9/vbc#VideoLearningMaterial">
